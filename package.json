{
  "name": "enigami-dashboard",
  "private": true,
  "version": "1.0.0",
  "main": "electron-main.js",
  "author": "Enigami",
  "description": "Dashboard de Gest√£o de Projetos de Arquitetura e Engenharia",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev:electron": "vite",
    "dist": "tsc && vite build && electron-builder"
  },
  "dependencies": {
    "@google/genai": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.enigami.dashboard",
    "productName": "ENIGAMI Dashboard",
    "files": [
      "dist/**/*",
      "electron-main.js"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
